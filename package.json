{
  "name": "sdupi-blockchain-explorer",
  "version": "1.0.0",
  "description": "SDUPI Blockchain Explorer - Etherscan-like functionality",
  "main": "blockchain-explorer-api.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:api\" \"pnpm run dev:frontend\"",
    "dev:api": "node blockchain-explorer-api.js",
    "dev:frontend": "cd \"sdupi-blockchain (1)\" && pnpm run dev",
    "start": "node blockchain-explorer-api.js",
    "build": "cd \"sdupi-blockchain (1)\" && pnpm run build",
    "install:all": "pnpm install && cd \"sdupi-blockchain (1)\" && pnpm install",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "blockchain",
    "explorer",
    "ethereum",
    "sdupi",
    "web3",
    "cryptocurrency",
    "blockchain-explorer",
    "etherscan"
  ],
  "author": "SDUPI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "ethers": "^6.8.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.4",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.2.0",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sdupi/blockchain-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/sdupi/blockchain-explorer/issues"
  },
  "homepage": "https://explorer.sdupi.com",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}